{
  "swagger": "2.0",
  "info": {
    "title": "repo/repo.proto",
    "version": "version not set"
  },
  "tags": [
    {
      "name": "Repo"
    }
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/api/repos": {
      "get": {
        "summary": "获取 repo 列表",
        "operationId": "Repo_List",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/repoListResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "required": false,
            "type": "string",
            "format": "int64"
          },
          {
            "name": "page_size",
            "in": "query",
            "required": false,
            "type": "string",
            "format": "int64"
          },
          {
            "name": "enabled",
            "in": "query",
            "required": false,
            "type": "boolean"
          }
        ],
        "tags": [
          "Repo"
        ]
      },
      "post": {
        "summary": "创建 repo",
        "operationId": "Repo_Create",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/repoCreateResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "name",
            "in": "query",
            "required": true,
            "type": "string"
          },
          {
            "name": "git_project_id",
            "in": "query",
            "required": false,
            "type": "string",
            "format": "int64"
          },
          {
            "name": "enabled",
            "in": "query",
            "required": false,
            "type": "boolean"
          },
          {
            "name": "mars_config.config_file",
            "description": "config_file 指定项目下的默认配置文件, 也可以是别的项目的文件，格式为 \"pid|branch|filename\"",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "mars_config.config_file_values",
            "description": "config_file_values 全局配置文件，如果没有 ConfigFile 则使用这个",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "mars_config.config_field",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "mars_config.is_simple_env",
            "in": "query",
            "required": false,
            "type": "boolean"
          },
          {
            "name": "mars_config.config_file_type",
            "description": "config_file_type 配置文件类型，php/env/yaml...",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "mars_config.local_chart_path",
            "description": "local_chart_path helm charts 目录, charts 文件在项目中存放的目录(必填), 也可以是别的项目的文件，格式为 \"pid|branch|path\"",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "mars_config.branches",
            "description": "branches 启用的分支",
            "in": "query",
            "required": false,
            "type": "array",
            "items": {
              "type": "string"
            },
            "collectionFormat": "multi"
          },
          {
            "name": "mars_config.values_yaml",
            "description": "values_yaml 和 values.yaml 一样",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "mars_config.display_name",
            "description": "显示的名称 (helm app name), 不填就使用 git server project name\n以字母开头结尾，中间可以有 '_' '-'",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "Repo"
        ]
      }
    },
    "/api/repos/toggle_enabled": {
      "post": {
        "summary": "开启/关闭",
        "operationId": "Repo_ToggleEnabled",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/repoToggleEnabledResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/repoToggleEnabledRequest"
            }
          }
        ],
        "tags": [
          "Repo"
        ]
      }
    },
    "/api/repos/{id}": {
      "get": {
        "summary": "获取 repo 详情",
        "operationId": "Repo_Show",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/repoShowResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string",
            "format": "int64"
          }
        ],
        "tags": [
          "Repo"
        ]
      }
    }
  },
  "definitions": {
    "googlerpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    },
    "marsConfig": {
      "type": "object",
      "properties": {
        "config_file": {
          "type": "string",
          "title": "config_file 指定项目下的默认配置文件, 也可以是别的项目的文件，格式为 \"pid|branch|filename\""
        },
        "config_file_values": {
          "type": "string",
          "title": "config_file_values 全局配置文件，如果没有 ConfigFile 则使用这个"
        },
        "config_field": {
          "type": "string"
        },
        "is_simple_env": {
          "type": "boolean"
        },
        "config_file_type": {
          "type": "string",
          "description": "config_file_type 配置文件类型，php/env/yaml..."
        },
        "local_chart_path": {
          "type": "string",
          "title": "local_chart_path helm charts 目录, charts 文件在项目中存放的目录(必填), 也可以是别的项目的文件，格式为 \"pid|branch|path\""
        },
        "branches": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "title": "branches 启用的分支"
        },
        "values_yaml": {
          "type": "string",
          "title": "values_yaml 和 values.yaml 一样"
        },
        "elements": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/marsElement"
          },
          "title": "elements 自定义字段"
        },
        "display_name": {
          "type": "string",
          "title": "显示的名称 (helm app name), 不填就使用 git server project name\n以字母开头结尾，中间可以有 '_' '-'"
        }
      }
    },
    "marsElement": {
      "type": "object",
      "properties": {
        "path": {
          "type": "string"
        },
        "type": {
          "$ref": "#/definitions/marsElementType"
        },
        "default": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "select_values": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "order": {
          "type": "integer",
          "format": "int64"
        }
      }
    },
    "marsElementType": {
      "type": "string",
      "enum": [
        "ElementTypeUnknown",
        "ElementTypeInput",
        "ElementTypeInputNumber",
        "ElementTypeSelect",
        "ElementTypeRadio",
        "ElementTypeSwitch",
        "ElementTypeTextArea",
        "ElementTypeNumberSelect",
        "ElementTypeNumberRadio"
      ],
      "default": "ElementTypeUnknown"
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "repoCreateResponse": {
      "type": "object",
      "properties": {
        "item": {
          "$ref": "#/definitions/typesRepoModel"
        }
      }
    },
    "repoListResponse": {
      "type": "object",
      "properties": {
        "page": {
          "type": "string",
          "format": "int64"
        },
        "page_size": {
          "type": "string",
          "format": "int64"
        },
        "count": {
          "type": "string",
          "format": "int64"
        },
        "items": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/typesRepoModel"
          }
        }
      }
    },
    "repoShowResponse": {
      "type": "object",
      "properties": {
        "item": {
          "$ref": "#/definitions/typesRepoModel"
        }
      }
    },
    "repoToggleEnabledRequest": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "format": "int64"
        },
        "enabled": {
          "type": "boolean"
        }
      },
      "required": [
        "id",
        "enabled"
      ]
    },
    "repoToggleEnabledResponse": {
      "type": "object",
      "properties": {
        "item": {
          "$ref": "#/definitions/typesRepoModel"
        }
      }
    },
    "typesRepoModel": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "format": "int64"
        },
        "git_project_id": {
          "type": "string",
          "format": "int64"
        },
        "enabled": {
          "type": "boolean"
        },
        "mars_config": {
          "$ref": "#/definitions/marsConfig"
        }
      }
    }
  }
