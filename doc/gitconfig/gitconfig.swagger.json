{
  "swagger": "2.0",
  "info": {
    "title": "gitconfig/gitconfig.proto",
    "version": "version not set"
  },
  "tags": [
    {
      "name": "GitConfig"
    }
  ],
  "consumes": [
    "application/json"
  ],
