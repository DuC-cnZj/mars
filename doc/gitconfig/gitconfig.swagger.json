    "application/json"
  ],
  "paths": {
    "/api/git/projects/{git_project_id}/default_values": {
      "get": {
        "summary": "GetDefaultChartValues 获取项目 helm charts 的默认 values.yaml",
        "operationId": "GitConfig_GetDefaultChartValues",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/gitconfigDefaultChartValuesResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "git_project_id",
            "in": "path",
            "required": true,
            "type": "string",
            "format": "int64"
          },
          {
            "name": "branch",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "GitConfig"
        ]
      }
    },
    "/api/git/projects/{git_project_id}/global_config": {
      "get": {
        "summary": "GlobalConfig 查看项目 GlobalConfig 配置",
        "operationId": "GitConfig_GlobalConfig",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/gitconfigGlobalConfigResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "git_project_id",
            "in": "path",
            "required": true,
            "type": "string",
            "format": "int64"
          }
        ],
        "tags": [
          "GitConfig"
        ]
      }
    },
    "/api/git/projects/{git_project_id}/mars_config": {
      "get": {
        "summary": "Show 查看项目配置",
        "operationId": "GitConfig_Show",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/gitconfigShowResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "git_project_id",
            "in": "path",
            "required": true,
            "type": "string",
            "format": "int64"
          },
          {
            "name": "branch",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "GitConfig"
        ]
      },
      "put": {
        "summary": "Update 更新全局配置",
        "operationId": "GitConfig_Update",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/gitconfigUpdateResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "git_project_id",
            "in": "path",
            "required": true,
            "type": "string",
            "format": "int64"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "config": {
                  "$ref": "#/definitions/marsConfig"
                }
              }
            }
          }
        ],
        "tags": [
          "GitConfig"
        ]
      }
    },
    "/api/git/projects/{git_project_id}/toggle_status": {
      "post": {
        "summary": "ToggleGlobalStatus 开启/关闭全局配置",
        "operationId": "GitConfig_ToggleGlobalStatus",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/gitconfigToggleGlobalStatusResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "git_project_id",
            "in": "path",
            "required": true,
            "type": "string",
            "format": "int64"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "enabled": {
                  "type": "boolean"
                }
              }
            }
          }
        ],
        "tags": [
          "GitConfig"
        ]
      }
    }
  },
  "definitions": {
    "gitconfigDefaultChartValuesResponse": {
      "type": "object",
      "properties": {
        "value": {
          "type": "string"
        }
      }
    },
    "gitconfigGlobalConfigResponse": {
      "type": "object",
      "properties": {
        "enabled": {
          "type": "boolean"
        },
        "config": {
          "$ref": "#/definitions/marsConfig"
        }
      }
    },
    "gitconfigShowResponse": {
      "type": "object",
      "properties": {
        "branch": {
          "type": "string"
        },
        "config": {
          "$ref": "#/definitions/marsConfig"
        }
      }
    },
    "gitconfigToggleGlobalStatusResponse": {
      "type": "object"
    },
    "gitconfigUpdateResponse": {
      "type": "object",
      "properties": {
        "config": {
          "$ref": "#/definitions/marsConfig"
        }
      }
    },
    "marsConfig": {
      "type": "object",
      "properties": {
        "config_file": {
          "type": "string",
          "title": "config_file 指定项目下的默认配置文件, 也可以是别的项目的文件，格式为 \"pid|branch|filename\""
        },
        "config_file_values": {
          "type": "string",
          "title": "config_file_values 全局配置文件，如果没有 ConfigFile 则使用这个"
        },
        "config_field": {
          "type": "string"
        },
        "is_simple_env": {
          "type": "boolean"
        },
        "config_file_type": {
          "type": "string",
          "description": "config_file_type 配置文件类型，php/env/yaml..."
        },
        "local_chart_path": {
          "type": "string",
          "title": "local_chart_path helm charts 目录, charts 文件在项目中存放的目录(必填), 也可以是别的项目的文件，格式为 \"pid|branch|path\""
        },
        "branches": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "title": "branches 启用的分支"
        },
        "values_yaml": {
          "type": "string",
          "title": "values_yaml 和 values.yaml 一样"
        },
        "elements": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/marsElement"
          },
          "title": "elements 自定义字段"
        },
        "display_name": {
          "type": "string",
          "title": "显示的名称 (helm app name), 不填就使用 git server project name\n以字母开头结尾，中间可以有 '_' '-'"
        }
      }
    },
    "marsElement": {
      "type": "object",
      "properties": {
        "path": {
          "type": "string"
        },
        "type": {
          "$ref": "#/definitions/marsElementType"
        },
        "default": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "select_values": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "marsElementType": {
      "type": "string",
      "enum": [
        "ElementTypeUnknown",
        "ElementTypeInput",
        "ElementTypeInputNumber",
        "ElementTypeSelect",
        "ElementTypeRadio",
        "ElementTypeSwitch",
        "ElementTypeTextArea"
      ],
      "default": "ElementTypeUnknown"
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "rpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    }
  }
